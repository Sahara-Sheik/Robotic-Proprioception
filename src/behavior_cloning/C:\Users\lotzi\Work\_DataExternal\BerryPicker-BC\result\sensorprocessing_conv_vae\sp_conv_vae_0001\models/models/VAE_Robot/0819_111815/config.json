{
    "name": "VAE_Robot",
    "n_gpu": 1,
    "arch": {
        "type": "VanillaVAE",
        "args": {
            "in_channels": 3,
            "latent_dims": 128,
            "flow": false
        }
    },
    "data_loader": {
        "type": "CelebDataLoader",
        "args": {
            "data_dir": "C:\\Users\\lotzi\\Work\\_DataExternal\\BerryPicker-BC\\result\\sensorprocessing_conv_vae\\sp_conv_vae_0001\\vae-training-data",
            "batch_size": 64,
            "shuffle": true,
            "validation_split": 0.2,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.005,
            "weight_decay": 0.0,
            "amsgrad": true
        }
    },
    "loss": "elbo_loss",
    "metrics": [],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 300,
        "save_dir": "C:\\Users\\lotzi\\Work\\_DataExternal\\BerryPicker-BC\\result\\sensorprocessing_conv_vae\\sp_conv_vae_0001\\models",
        "save_period": 5,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}